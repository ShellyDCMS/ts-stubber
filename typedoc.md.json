{
  "$schema": "https://typedoc.org/schema.json",
  "plugin": ["typedoc-plugin-markdown", "typedoc-plugin-merge-modules"],
  "jsDocCompatibility": {
    "exampleTag": true,
    "defaultTag": true
  },
  "objectLiteralTypeDeclarationStyle": "list",
  "entryPoints": ["./src/index.ts"],
  "out": "docs",
  "disableSources": true,
  "githubPages": false
}
